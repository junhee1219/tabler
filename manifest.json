{
  "manifest_version": 3,
  "name": "Tabler Exporter",
  "version": "1.0.0",
  "permissions": ["activeTab","scripting","downloads"],
  "host_permissions": ["*://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "src/popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": [
      "src/content.js"
    ],
      "run_at": "document_idle"
    }
  ]
}
