{
  "manifest_version": 3,
  "name": "Tabler Exporter",
  "version": "1.0.0",
  "permissions": ["activeTab","scripting","downloads"],
  "host_permissions": ["*://*/*"],
  "action": {
    "default_popup": "src/popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": [
        "libs/exceljs.browser.js",
        "libs/FileSaver.min.js"
      ],
      "run_at": "document_idle"
    }
  ]
}
